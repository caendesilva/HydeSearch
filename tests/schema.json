{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "array",
  "default": [],
  "title": "Root Schema",
  "items": {
    "type": "object",
    "title": "A Schema",
    "required": [
      "slug",
      "title",
      "content",
      "destination"
    ],
    "properties": {
      "slug": {
        "type": "string",
        "title": "The slug Schema",
        "examples": [
          "down-the-rabbithole"
        ]
      },
      "title": {
        "type": "string",
        "title": "The title Schema",
        "examples": [
          "Down the Rabbit-Hole"
        ]
      },
      "content": {
        "type": "string",
        "title": "The content Schema",
        "examples": [
          "CHAPTER I\n\n Down the Rabbit-Hole\n\n Alice was beginning to get very tired [...]"
        ]
      },
      "destination": {
        "type": "string",
        "title": "The destination Schema",
        "examples": [
          "down-the-rabbithole.html"
        ]
      }
    }
  }
}